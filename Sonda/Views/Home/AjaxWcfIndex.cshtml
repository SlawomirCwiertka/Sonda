<script src="~/Scripts/jquery-2.1.3.min.js"></script>
<h2>AjaxWcf</h2>
<div id="content">

</div>
<script>
    $(document).ready(function () {
        get();
    })

    function get(){
        $.ajax({
            type: 'GET',
            url: "AjaxWcfGet",
            success: function (data) {
                $('#content').html(data);
            }
        })
    };
</script>